<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luis.appprueba">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dinerapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/type_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewUserActivity"
            android:label="@string/type_newuser" />
        <activity
            android:name=".MainMenuActivity"
            android:label="Main Menu" />
        <activity
            android:name=".NewEventActivity"
            android:label="@string/type_newevent" />
        <activity
            android:name=".UserProfile"
            android:label="@string/type_userprofile" />
    </application>

</manifest>
